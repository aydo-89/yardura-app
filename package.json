{
  "name": "yarddog-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "worker": "tsx workers/sampleScorer.ts",
    "build": "npx prisma generate && npm run type-check && next build --no-lint",
    "build:quick": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "perf:lh": "lhci autorun --config=.lighthouserc.json",
    "perf:lh:mobile": "lhci autorun --config=.lighthouserc.json --preset=mobile",
    "perf:analyze": "ANALYZE=true next build",
    "perf:audit": "npm run perf:lh && npm run perf:analyze",
    "design-system:check": "tsx scripts/design-system-guard.ts",
    "design-system:fix": "npm run lint:fix && npm run design-system:check",
    "og:preview": "echo 'Open these URLs in your browser:' && echo 'http://localhost:3000/api/og?type=homepage' && echo 'http://localhost:3000/api/og?type=quote' && echo 'http://localhost:3000/api/og?type=insights&title=Test%20Article' && echo 'http://localhost:3000/api/og?type=facts'",
    "og:test": "curl -s http://localhost:3000/api/og?type=homepage | head -5 && echo '\\n---\\n' && curl -s http://localhost:3000/api/og?type=quote | head -5",
    "hero:video": "node create-hero-video.js",
    "hero:update": "node update-hero-to-video.js",
    "hero:revert": "node update-hero-to-video.js revert",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:smoke": "node scripts/smoke-tests.js",
    "setup": "npm run db:setup",
    "db:setup": "npx prisma generate && npx prisma db push",
    "db:reset": "npx prisma db push --force-reset",
    "db:seed": "node scripts/seed.js development",
    "db:seed:staging": "node scripts/seed.js staging",
    "db:seed:production": "node scripts/seed.js production",
    "migrate": "node infra/migration/migrate.js full",
    "migrate:export": "node infra/migration/migrate.js export",
    "migrate:import": "node infra/migration/migrate.js import",
    "migrate:validate": "node infra/migration/migrate.js validate",
    "test:dual-db": "node infra/test-dual-db.js",
    "build:tiles": "tsx scripts/build-tiles.ts",
    "docker:build": "docker build -t yardura-web .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f web",
    "docker:dev": "docker-compose -f docker-compose.yml up --build",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build -d",
    "docker:clean": "docker-compose down -v --rmi all"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@google-cloud/recaptcha-enterprise": "^6.3.0",
    "@hookform/resolvers": "^5.2.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.11.0",
    "@stripe/react-stripe-js": "^3.9.1",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/supabase-js": "^2.56.0",
    "@turf/turf": "^7.2.0",
    "@turf/unkink-polygon": "^7.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/geojson": "^7946.0.16",
    "@vercel/analytics": "^1.5.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.58.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "comlink": "^4.4.2",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.12",
    "geojson-rbush": "^3.2.0",
    "idb-keyval": "^6.2.2",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.540.0",
    "maplibre-gl": "^5.7.1",
    "next": "^15.5.2",
    "next-auth": "^4.24.11",
    "next-rate-limit": "^0.0.3",
    "next-seo": "^6.8.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "pdf-lib": "^1.17.1",
    "pmtiles": "^4.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.62.0",
    "react-imask": "^7.6.1",
    "react-intersection-observer": "^9.16.0",
    "recharts": "^2.12.7",
    "redis": "^5.8.2",
    "resend": "^6.0.3",
    "shapefile": "^0.6.6",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0",
    "swr": "^2.3.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.5.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/google.maps": "^3.58.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.14",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.2",
    "lint-staged": "^16.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "qrcode": "1.5.4",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
